{
  "files.associations": {
    "*.h": "cpp",
    "*.sass": "sass",
    "*.svg": "html",
    "**/include/**": "cpp",
    "array": "cpp",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "exception": "cpp",
    "functional": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "cmath": "cpp",
    "csetjmp": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "*.ipp": "cpp",
    "*.inc": "c"
  },
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.configurationWarnings": "Disabled",
  "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c11",
  "git.detectSubmodules": false,
  "files.exclude": {
    "**/.gitmodules": false
  },
  "search.exclude": {
    "**/build": true,
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/external_libs": true,
    "**/platform/particle/eGUI": true,
    "**/platform/particle/WebSockets": true,
  },
  "shellformat.effectLanguages": [
    "shellscript"
  ],
  "[dockerfile]": {
    "editor.formatOnSave": false
  },
  "remote.localPortHost": "allInterfaces",
  "disasexpl.associations": {
    "**/*.c": "${workspaceFolder}/build/target/user/platform-8-m/firmware/${relativeFileDirname}/${fileBasenameNoExtension}.s",
    "**/*.cpp": "${workspaceFolder}/build/target/user/platform-8-m/firmware/${relativeFileDirname}/${fileBasenameNoExtension}.s"
  },
  "cmake.configureOnOpen": false,
  "C_Cpp.codeAnalysis.clangTidy.enabled": true,
  "C_Cpp.codeAnalysis.clangTidy.headerFilter": ".*",
  "C_Cpp.codeAnalysis.clangTidy.checks.enabled": [
    // "clang-analyzer-*",
    // "cppcoreguidelines-*",
    // "modernize-*",
    "google-explicit-constructor",
  ],
  "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
    "cppcoreguidelines-pro-bounds-constant-array-index",
    "cppcoreguidelines-pro-bounds-array-to-pointer-decay",
    "modernize-use-trailing-return-type"
  ],
  "C_Cpp.codeAnalysis.exclude": {
    "**/external_libs": true,
    "**/build": true,
    "**/app/brewblox-esp": true,
    "**/platform/particle": true
  },
}

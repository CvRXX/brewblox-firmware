{
  "files.associations": {
    "**/include/**": "cpp",
    "array": "cpp",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "exception": "cpp",
    "functional": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "typeinfo": "cpp",
    "cmath": "cpp",
    "csetjmp": "cpp",
    "csignal": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "*.ipp": "cpp"
  },
  "C_Cpp.formatting": "clangFormat",
  "C_Cpp.configurationWarnings": "Disabled",
  "C_Cpp.default.browse.limitSymbolsToIncludedHeaders": true,
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c11",
  "git.detectSubmodules": false,
  "files.exclude": {
    "**/.gitmodules": false
  },
  "shellformat.effectLanguages": [
    "shellscript"
  ],
  "[dockerfile]": {
    "editor.formatOnSave": false
  },
  "debug.onTaskErrors": "debugAnyway"
}
